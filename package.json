{
  "name": "nextjs-social-media",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently --kill-others \"next dev\" \"vitest watch\"",
    "httpsdev": "node server.js",
    "build": "dotenv -e .env -- prisma db push --force-reset && dotenv -e .env -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest test",
    "cy:open": "cypress open",
    "cy:api": "cypress run --headed --no-exit --spec \"src/**/*.api.cy.ts\""
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.2",
    "@tanstack/react-query": "^4.29.19",
    "@tensorflow/tfjs": "^4.8.0",
    "@types/cookies": "^0.7.7",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "argon2": "^0.30.3",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "cloudinary": "^1.37.3",
    "cookies": "^0.8.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "json-bigint": "^1.0.0",
    "multer": "1.4.5-lts.1",
    "next": "13.4.3",
    "next-auth": "^4.22.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-datepicker": "^4.14.1",
    "react-dom": "18.2.0",
    "react-drag-drop-files": "^2.3.10",
    "react-hook-form": "^7.45.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.14.0",
    "react-select": "^5.7.3",
    "react-test-renderer": "^18.2.0",
    "reactjs-popup": "^2.0.5",
    "streamifier": "^0.1.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/react": "^14.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/json-bigint": "^1.0.1",
    "@types/multer": "^1.4.7",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/streamifier": "^0.1.0",
    "concurrently": "^8.2.0",
    "cypress": "^12.16.0",
    "jsdom": "^22.1.0",
    "prisma": "^4.16.2",
    "tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  }
}
